  "main": "src/server/server.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch \"src/\" --exec \"ts-node ./src/core/server/server.ts\" -e ts",
    "u-test": "cross-env NODE_ENV=test mocha --opts ./dist/tests/unit/config/mocha.opts ./dist/tests/unit/unit.test.js --exit",
    "i-test": "cross-env NODE_ENV=test mocha --opts ./dist/tests/integration/config/mocha.opts ./dist/tests/integration/integration.test.js --exit",
    "u-cover": "nyc --reporter=lcov --reporter=text-summary npm run u-test",
    "i-cover": "nyc --reporter=lcov --reporter=text-summary npm run i-test"
  },
  "keywords": [
    "API",
    "TypeScript",
